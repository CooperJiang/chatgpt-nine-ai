{
    "apps": {
        "name": "nineai-open",
        "script": "./dist/main.js",
        "watch": true,
        "ignore_watch": [
            "node_modules",
            "logs"
        ],
				"env": {
					"TZ": "Asia/Shanghai"
				},
        "instances": 1,
        "error_file": "logs/err.log",
        "out_file": "logs/out.log",
        "log_date_format": "YYYY-MM-DD HH:mm:ss"
    }
}
